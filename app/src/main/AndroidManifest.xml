<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vos.statisitk">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_home_statistic"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".TableCalculationActivity"></activity>
        <activity
            android:name=".CalculationActivity"
            android:parentActivityName=".NumberSizeActivity" />
        <activity android:name=".NumberSizeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>